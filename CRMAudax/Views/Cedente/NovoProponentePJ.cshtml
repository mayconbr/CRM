                    <input type="text"
                           name="cpfCnpj"
                           id="cpfCnpj"
                           placeholder="Ex: 000.000.000 - 00"
                           required />
                    <p id="validacaodocnpj"></p>
                </div>
            </div>
            <div class="object-input">
                <div>
                    <label>Nome Fantasia *</label>
                </div>
                <div>
                    <input type="text"
                           name="nome"
                           id="nome"
                           placeholder="Digite o nome do proponente"
                           required />
                </div>
            </div>
            <div>
                <div>
                    <label>Razão Social *</label>
                </div>
