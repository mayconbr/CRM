@{
    ViewData["Title"] = "Cadastro de Proponente";
}

<div class="form-style">
    <h3 class="titulo-pagina">Cadastro de Proponente</h3>
    <div class="linha-horizontal"></div>
    <form onsubmit="doNovoProponente(this); return false;" id="doNovoProponente">
        <div class="pjpf">
            <input type="radio" id="TipoPessoa" name="TipoPesssoa" onclick="window.location.href = this.checked ? '/Cedente/NovoProponentePF' : '';" value="1" />
            <label class="pf" for="PF">Pessoa Física</label>
            <input type="radio" id="TipoPessoa" name="TipoPesssoa" value="2" checked />
            <label class="pj" for="PJ">Pessoa Juridíca</label>
