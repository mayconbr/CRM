                u.balanco = Boolean($("input[type='radio'][name='balanco']:checked").val());
                u.tempoFundacao = Boolean($("input[type='radio'][name='tempoFundacao']:checked").val());
                u.mudancaAtividade = Boolean($("input[type='radio'][name='mudancaAtividade']:checked").val());
