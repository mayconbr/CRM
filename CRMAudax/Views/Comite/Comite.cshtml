                u.limiteAcima = Boolean($("input[type='radio'][name='limiteAcima']:checked").val());
                u.concentracaoSacados = Boolean($("input[type='radio'][name='concentracaoSacados']:checked").val());
                u.antecipacaoImoveis = Boolean($("input[type='radio'][name='antecipacaoImoveis']:checked").val());
                u.entregaMercadoria = Boolean($("input[type='radio'][name='entregaMercadoria']:checked").val());
                u.confirmaNota = Boolean($("input[type='radio'][name='confirmaNota']:checked").val());
                u.consultaAcimaMedia = Boolean($("input[type='radio'][name='consultaAcimaMedia']:checked").val());
                u.consultaCnpj = Boolean($("input[type='radio'][name='consultaCnpj']:checked").val());
